{
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/_next/static/*",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/api/*",
            "/_next/*",
            "/favicon.ico",
            "/*.{css,js,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}"
        ]
    },
    "mimeTypes": {
        ".json": "application/json"
    },
    "globalHeaders": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
    }
}